<?xml version="1.0" encoding="UTF-8"?>
<form>
	<fieldset name="scenes_hotspots" label="MOD_TOURHOTSPOTS_LBL" description="MOD_TOURHOTSPOTS_LBL_DESC" >
		<field type="hidden" name="tourhotspots_type" value="info" />
		<field type="text" label="MOD_TOURHOTSPOTS_PITCH" name="tourhotspots_pitch" 
			description="MOD_TOURHOTSPOTS_PITCH_DESC" default="14.1" class="input-mini" />
		<field type="text" label="MOD_TOURHOTSPOTS_YAW" name="tourhotspots_yaw" 
			description="MOD_TOURHOTSPOTS_YAW_DESC" default="1.5" class="input-mini" />
		<field type="text" label="MOD_TOURHOTSPOTS_TEXT" name="tourhotspots_text" 
			description="MOD_TOURHOTSPOTS_TEXT_DESC" default="Info button with test link to pannellum" 
			class="input-xlarge" size="32" maxlength="256" filter="" />
		<field type="url" label="MOD_TOURHOTSPOTS_URL" name="tourhotspots_url" 
			description="MOD_TOURHOTSPOTS_URL_DESC" default="https://pannellum.org/" 
			class="input-xlarge" size="32" maxlength="256" filter="" />
	</fieldset>
</form>